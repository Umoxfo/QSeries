<?xml version="1.0"?>
<!--For -100 -200 variants-->
<PropertyList>
	<fx>
		<!--Battery Switch, Cabincall, no smoking sign, seatbelt sign; code from 777, audio produced with audactiy-->
		<chime>
			<name>nosmoking</name>
			<mode>once</mode>
			<type>avionics</type>
			<path>cabincall.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/pass-warn</property>
						<value>15</value>
					</greater-than>
					<property>controls/cabin/nosmoking</property>
				</and>
			</condition>
		</chime>
		<chime>
			<name>nosmoking1</name>
			<mode>once</mode>
			<type>avionics</type>
			<path>cabincall.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/pass-warn</property>
						<value>15</value>
					</greater-than>
					<not>
						<property>controls/cabin/nosmoking</property>
					</not>
				</and>
			</condition>
		</chime>
		<chime>
			<name>seatbelt</name>
			<mode>once</mode>
			<type>avionics</type>
			<path>cabincall.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/pass-warn</property>
						<value>15</value>
					</greater-than>
					<property>controls/cabin/seatbelts</property>
				</and>
			</condition>
		</chime>
		<chime>
			<name>seatbelt1</name>
			<mode>once</mode>
			<type>avionics</type>
			<path>cabincall.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/pass-warn</property>
						<value>15</value>
					</greater-than>
					<not>
						<property>controls/cabin/seatbelts</property>
					</not>
				</and>
			</condition>
		</chime>
		<!-- APU, same for inside and outside; APU sound and config from CRJ700-family licensed under GPL -->
		<apu>
			<name>APU starting</name>
			<mode>once</mode>
			<path>apu_start.wav</path>
			<condition>
				<property>controls/APU/start</property>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.8</x>
				<y>0.0</y>
				<z>0.8</z>
			</position>
		</apu>
		<apu>
			<name>APU running</name>
			<mode>looped</mode>
			<path>apu_run.wav</path>
			<condition>
				<property>controls/APU/running</property>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.8</x>
				<y>0.0</y>
				<z>0.8</z>
			</position>
		</apu>
		<apu>
			<name>APU shutdown</name>
			<mode>once</mode>
			<path>apu_shutdown.wav</path>
			<condition>
				<and>
					<not>
						<property>controls/APU/master</property>
					</not>
				</and>
			</condition>
			<reference-dist>10</reference-dist>
			<max-dist>50</max-dist>
			<position>
				<x>13.8</x>
				<y>0.0</y>
				<z>0.8</z>
			</position>
		</apu>
		<!--Gear Warning Horn see http://www.smartcockpit.com/docs/Dash8-200-300-Landing_Gear.pdf p.1-->
		<horn>
			<name>gearhorn</name>
			<mode>looped</mode>
			<type>avionics</type>
			<path>gear_horn.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/gear-horn</property>
						<value>15</value>
					</greater-than>
					<not-equals>
						<property>/gear/warning-horn</property>
						<value>0</value>
					</not-equals>
				</and>
			</condition>
			<reference-dist>1</reference-dist>
			<max-dist>3</max-dist>
			<position>
				<x>-5</x>
				<y>-0.1</y>
				<z>1</z>
			</position>
		</horn>
		<!--Takeoff Warning Horn see http://www.smartcockpit.com/docs/Dash8-200-300-Warning_Systems.pdf p.18-->
		<horn>
			<name>tohorn</name>
			<mode>looped</mode>
			<type>avionics</type>
			<path>to_horn.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/to-horn</property>
						<value>15</value>
					</greater-than>
					<property>gear/gear[1]/wow</property>
					<or>
						<greater-than-equals>
							<property>controls/engines/engine[0]/throttle</property>
							<value>0.5</value>
						</greater-than-equals>
						<greater-than-equals>
							<property>controls/engines/engine[1]/throttle</property>
							<value>0.5</value>
						</greater-than-equals>
					</or>
					<or>
						<or>
							<greater-than>
								<property>controls/flight/elevator-trim</property>
								<value>0.0</value>
							</greater-than>
							<less-than>
								<property>controls/flight/elevator-trim</property>
								<value>-0.3</value>
							</less-than>
						</or>
						<or>
							<less-than>
								<property>controls/flight/flaps</property>
								<value>0.14</value>
							</less-than>
							<greater-than>
								<property>controls/flight/flaps</property>
								<value>0.43</value>
							</greater-than>
						</or>
						<or>
							<not-equals>
								<property>controls/engines/engine[0]/condition-lever-state</property>
								<value>2</value>
							</not-equals>
							<not-equals>
								<property>controls/engines/engine[0]/condition-lever-set</property>
								<value>1</value>
							</not-equals>
							<not-equals>
								<property>controls/engines/engine[1]/condition-lever-state</property>
								<value>2</value>
							</not-equals>
							<not-equals>
								<property>controls/engines/engine[1]/condition-lever-set</property>
								<value>1</value>
							</not-equals>
						</or>
						<property>controls/gear/brake-parking</property>
					</or>
				</and>
			</condition>
			<reference-dist>1</reference-dist>
			<max-dist>3</max-dist>
			<position>
				<x>-5</x>
				<y>-0.1</y>
				<z>1</z>
			</position>
		</horn>
		<!--Overspeed Horn see http://www.smartcockpit.com/docs/Dash8-200-300-Warning_Systems.pdf p.20-->
		<horn>
			<name>overspeedhorn</name>
			<mode>looped</mode>
			<type>avionics</type>
			<path>to_horn.wav</path>
			<condition>
				<and>
					<greater-than>
						<property>/systems/electrical/outputs/ovspd-horn</property>
						<value>15</value>
					</greater-than>
					<or>
						<property>/instrumentation/ovspd-horn/active</property>
						<not-equals>
							<property>/instrumentation/ovspd-horn/test</property>
							<value>0</value>
						</not-equals>
					</or>
				</and>
			</condition>
			<reference-dist>1</reference-dist>
			<max-dist>3</max-dist>
			<position>
				<x>-5</x>
				<y>-0.1</y>
				<z>1</z>
			</position>
		</horn>
