<?xml version="1.0"?>
<PropertyList>
	<path>services-200.ac</path>
	<animation>
		<type>select</type>
		<condition>
			<property>/services/bus/enable</property>
		</condition>
		<object-name>bus</object-name>
	</animation>
	<animation>
		<type>select</type>
		<condition>
			<property>/services/baggage-cart/enable</property>
		</condition>
		<object-name>baggage_cart1</object-name>
		<object-name>baggage_cart2</object-name>
	</animation>
	<animation>
		<type>select</type>
		<condition>
			<property>/services/fuel-truck/enable</property>
		</condition>
		<object-name>fuel_truck</object-name>
		<object-name>fuel_pipe</object-name>
		<object-name>fuel_pipe_disconnected</object-name>
	</animation>
	<animation>
		<type>select</type>
		<condition>
			<property>/services/safety-cone/enable</property>
		</condition>
		<object-name>safety-cone</object-name>
	</animation>
</PropertyList>
