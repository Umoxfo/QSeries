<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>interior-200.ac</path>
	
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>1000</max-m>
		<max-property>sim/rendering/static-lod/bare</max-property>
	</animation>

	
	
	<effect>
		<inherits-from>Aircraft/Q400/Models/Effects/panel</inherits-from>
		<object-name>flightdeckdoor</object-name>
		<object-name>cockpit</object-name>
		<object-name>cockpit_wall</object-name>
	</effect>
	
	<animation>
		<type>pick</type>
		<object-name>flightdeckdoor</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>doors.fd.toggle();</script>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>flightdeckdoor</object-name>
		<property>sim/model/door-positions/fd/position-norm</property>
		<interpolation>
			<entry><ind> 0 </ind><dep>  0 </dep></entry>
			<entry><ind> 1 </ind><dep> 80 </dep></entry>
		</interpolation>
		<center>
			<x-m> -10.76787 </x-m>
			<y-m>  0.31383 </y-m>
		</center>
		<axis>
			<x> 0 </x>
			<y> 0 </y>
			<z> 1 </z>
		</axis>
	</animation>
  
</PropertyList>

