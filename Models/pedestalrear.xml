<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	<path>pedestalrear.ac</path>
        
        
        <!--Panel illumination-->
    <effect>
        <inherits-from>Effects/panel-lighting</inherits-from>
        <parameters>
            <implicit-lightmap-intensity><use>/systems/electrical/outputs/centerpanel-lights</use></implicit-lightmap-intensity>
        </parameters>
	<object-name>base.1</object-name>
	<object-name>base.2</object-name>
	<object-name>base.3</object-name>
	<object-name>base.4</object-name>
	<object-name>base.5</object-name>
	<object-name>screws</object-name>
    </effect>
        
      
        
        <!--TRIM PANEL-->
<!--Rudder Trim-->

  <animation>
        <type>knob</type>
        <object-name>ruddertrim</object-name>
                <property>/controls/flight/rudder-trim</property>
	<factor>-360</factor>
    <center>
      <x-m> -11.68977</x-m>
      <y-m>  0.03235 </y-m>
    </center>
	<axis><z>1</z></axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/flight/rudder-trim</property>
                <factor>0.01</factor>
                <min>-0.2</min>
                <max>0.2</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

  <animation>
        <type>rotate</type>
        <object-name>ruddertrimind</object-name>
                <property>/controls/flight/rudder-trim</property>
	<factor>-360</factor>
    <center>
      <x-m> -11.72675</x-m>
      <y-m>  0.03195 </y-m>
    </center>
	<axis><z>1</z></axis>
    </animation>

  <animation>
        <type>knob</type>
        <object-name>ailerontrim</object-name>
                <property>/controls/flight/aileron-trim</property>
	<factor>-360</factor>
    <center>
      <y-m> -0.03611</y-m>
      <z-m>  1.86654 </z-m>
    </center>
	<axis><x>1</x></axis>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/controls/flight/aileron-trim</property>
                <factor>0.01</factor>
                <min>-0.2</min>
                <max>0.2</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

  <animation>
        <type>rotate</type>
        <object-name>ailerontrimind</object-name>
                <property>/controls/flight/aileron-trim</property>
	<factor>-360</factor>
    <center>
      <x-m>-11.7277</x-m>
      <y-m>  -0.03483</y-m>
    </center>
	<axis><z>1</z></axis>
    </animation>
    
    
    <!--A-TEAM COMM/NAV/ATC-->
    <animation>
        <type>select</type>
        <object-name>team.screen.off</object-name>
        <condition>
            <equals>
                <property>/instrumentation/team/running</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>team.screen</object-name>
        <object-name>vhf1.act.100</object-name>
        <object-name>vhf1.act.10</object-name>
        <object-name>vhf1.act.1</object-name>
        <object-name>vhf1.act.point</object-name>
        <object-name>vhf1.act.01</object-name>
        <object-name>vhf1.act.001</object-name>
        <object-name>vhf1.act.0001</object-name>
        <object-name>vhf1.sby.100</object-name>
        <object-name>vhf1.sby.10</object-name>
        <object-name>vhf1.sby.1</object-name>
        <object-name>vhf1.sby.point</object-name>
        <object-name>vhf1.sby.01</object-name>
        <object-name>vhf1.sby.001</object-name>
        <object-name>vhf1.sby.0001</object-name>        
        <object-name>vhf2.act.100</object-name>
        <object-name>vhf2.act.10</object-name>
        <object-name>vhf2.act.1</object-name>
        <object-name>vhf2.act.point</object-name>
        <object-name>vhf2.act.01</object-name>
        <object-name>vhf2.act.001</object-name>
        <object-name>vhf2.act.0001</object-name>
        <object-name>vhf2.sby.100</object-name>
        <object-name>vhf2.sby.10</object-name>
        <object-name>vhf2.sby.1</object-name>
        <object-name>vhf2.sby.point</object-name>
        <object-name>vhf2.sby.01</object-name>
        <object-name>vhf2.sby.001</object-name>
        <object-name>vhf2.sby.0001</object-name>
        <condition>
            <equals>
                <property>/instrumentation/team/running</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>team.screen.test</object-name>
        <condition>
            <equals>
                <property>/instrumentation/team/running</property>
                <value>2</value>
            </equals>
        </condition>
    </animation>
    
    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>vhf1.act.100</object-name>
        <object-name>vhf1.act.10</object-name>
        <object-name>vhf1.act.1</object-name>
        <object-name>vhf1.act.point</object-name>
        <object-name>vhf1.act.01</object-name>
        <object-name>vhf1.act.001</object-name>
        <object-name>vhf1.act.0001</object-name>
        <object-name>vhf1.sby.100</object-name>
        <object-name>vhf1.sby.10</object-name>
        <object-name>vhf1.sby.1</object-name>
        <object-name>vhf1.sby.point</object-name>
        <object-name>vhf1.sby.01</object-name>
        <object-name>vhf1.sby.001</object-name>
        <object-name>vhf1.sby.0001</object-name>        
        <object-name>vhf2.act.100</object-name>
        <object-name>vhf2.act.10</object-name>
        <object-name>vhf2.act.1</object-name>
        <object-name>vhf2.act.point</object-name>
        <object-name>vhf2.act.01</object-name>
        <object-name>vhf2.act.001</object-name>
        <object-name>vhf2.act.0001</object-name>
        <object-name>vhf2.sby.100</object-name>
        <object-name>vhf2.sby.10</object-name>
        <object-name>vhf2.sby.1</object-name>
        <object-name>vhf2.sby.point</object-name>
        <object-name>vhf2.sby.01</object-name>
        <object-name>vhf2.sby.001</object-name>
        <object-name>vhf2.sby.0001</object-name>
    </effect>
        
    
    <!--MFD CONTROL PANEL-->
  <animation>
        <type>pick</type>
        <object-name>MFD.elec</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/mfd/inputs/sys-page</property>
		<value>elec</value>
            </binding>
        </action>
    </animation>
  <animation>
        <type>pick</type>
        <object-name>MFD.eng</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/mfd/inputs/sys-page</property>
		<value>eng</value>
            </binding>
        </action>
    </animation>
  <animation>
        <type>pick</type>
        <object-name>MFD.fuel</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/mfd/inputs/sys-page</property>
		<value>fuel</value>
            </binding>
        </action>
    </animation>
  <animation>
        <type>pick</type>
        <object-name>MFD.doors</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-assign</command>
                <property>/instrumentation/mfd/inputs/sys-page</property>
		<value>doors</value>
            </binding>
        </action>
    </animation>
  <animation>
        <type>pick</type>
        <object-name>MFD.all</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>/instrumentation/mfd/inputs/sys-page</property>
		<value>eng</value>
		<value>fuel</value>
		<value>doors</value>
		<value>elec</value>
            </binding>
        </action>
    </animation>
  <animation>
        <type>pick</type>
        <object-name>MFD1.mainpage</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>/instrumentation/mfd/inputs/main-page</property>
		<value>pfd</value>
		<value>sys</value>
		<value>ed</value>
            </binding>
        </action>
    </animation>
    
    <!--LEFT TEAM-->
  <animation>
        <type>pick</type>
        <object-name>leftteam.button1</object-name>
        <action>
            <button>0</button>
            <repeatable>0</repeatable>
            <binding>
		    <condition>
				    <property>/instrumentation/comm[0]/selected</property>
		    </condition>
			<command>property-swap</command>
			<property>instrumentation/comm[0]/frequencies/selected-mhz</property>
			<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
            </binding>
            <binding>
		    <condition>
			    <not>
				    <property>/instrumentation/comm[0]/selected</property>
			    </not>
		    </condition>
                <command>property-toggle</command>
		<property>/instrumentation/comm[0]/selected</property>
            </binding>
            <binding>
                <command>property-toggle</command>
		<property>/instrumentation/comm[0]/activity</property>
            </binding>
        </action>
    </animation>
  <animation>
    <type>knob</type>
    <object-name>leftteam.tune.khz</object-name>
  <!--  <property alias="../../params/nav-frq-standby"/>
    <factor>-360</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.0320882</x-m>
      <y-m>0.058</y-m>
      <z-m>-0.012</z-m>
    </center>-->
    <action>
      <binding>
        <condition>
		<property>instrumentation/comm[0]/selected</property>
        </condition>
        <command>property-adjust</command>
	<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
        <factor>0.05</factor>
        <min>0.0</min>
        <max>1.0</max>
        <wrap>true</wrap>
        <mask>decimal</mask>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>leftteam.tune.mhz</object-name>
    <!--<property alias="../../params/nav-frq-standby"/>
    <factor>-7</factor>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0.0320882</x-m>
      <y-m>0.058</y-m>
      <z-m>-0.012</z-m>
    </center>-->
    <action>
      <binding>
        <condition>
		<property>instrumentation/comm[0]/selected</property>
        </condition>
        <command>property-adjust</command>
	<property>instrumentation/comm[0]/frequencies/standby-mhz</property>
        <factor>1</factor>
        <min>108.0</min>
        <max>118.0</max>
        <wrap>true</wrap>
      </binding>
    </action>
  </animation>
    
    
</PropertyList>
