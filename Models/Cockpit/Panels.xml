<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
	
	
	<!--Overhead Panel-->
	<model>
	<path>Overhead/overhead.xml</path>
	</model>
	
	
	<!--Glareshield Panel-->
	<model>
	<path>Glareshield/glareshieldpanel.xml</path>
	</model>


	<model>
		<name>HUD</name>
		<path>HUD/hud.xml</path>
	</model>
	<animation>
		<type>select</type>
		<object-name>HUD</object-name>
		<condition>
			<or>
				<property>/sim/model/livery/enable-hud</property>
				<property>/sim/model/always-show-hud</property>
			</or>
		</condition>
	</animation>
	<model>
		<path>Instruments/PFD/PFD.pilot.xml</path>
		<offsets>
			<x-m>-12.31</x-m>
			<y-m>-0.48</y-m>
			<z-m>2.125</z-m>
			<pitch-deg>-16.2</pitch-deg>
		</offsets>
	</model>
	<model>
		<path>Instruments/PFD/PFD.copilot.xml</path>
		<offsets>
			<x-m>-12.31</x-m>
			<y-m>0.48</y-m>
			<z-m>2.125</z-m>
			<pitch-deg>-16.2</pitch-deg>
		</offsets>
	</model>
	<!--NDs--><!--
	<model>
	<path>panels/ND/ND.xml</path>
	</model>-->
	<!--CDUs-->
	<model>
		<path>Pedestal/Front/CDU/CDU.xml</path>
		<offsets>
			<x-m>-12.2019</x-m>
			<y-m>-0.145</y-m>
			<z-m>1.95</z-m>
			<pitch-deg>-60</pitch-deg>
		</offsets>
	</model>
	<model>
		<path>Pedestal/Front/CDU/CDU1.xml</path>
		<offsets>
			<x-m>-12.2019</x-m>
			<y-m>0.145</y-m>
			<z-m>1.95</z-m>
			<pitch-deg>-60</pitch-deg>
		</offsets>
	</model>

	<!--Engine Instrument-->
	<model>
		<path>Instruments/ED/ED.xml</path>
	</model>
	
	<!--Multi-Functional Flight Display (System pages/Nav Display)-->
	<model>
		<path>Instruments/MFD/MFD.pilot.xml</path>
	</model>
	<model>
		<path>Instruments/MFD/MFD.copilot.xml</path>
	</model>

	<!--Landing Gear Panel-->
	<model>
		<path>MainPanel/lg/lg.xml</path>
	</model>

	<!--Integrated Electronic Standby Instrument-->
	<model>
		<path>Instruments/IESI/IESI.xml</path>
	</model>
	
	<!--Pilot's/Copilot's side panel-->
	<model>
		<path>MainPanel/baropanel/baropanel.xml</path>
	</model>
	<model>
		<path>MainPanel/baropanel/baropanel.xml</path>
		<offsets>
			<y-m>1.27788</y-m>
		</offsets>
	</model>

	<!--Control Yokes-->
	<model>
		<path>Yoke/yoke.xml</path>
		<name>yoke</name>
	</model>

	<!--PEDESTAL-->
	<model>
		<path>Pedestal/Front/frontpedestal.xml</path>
	</model>
	<model>
		<path>Pedestal/Center/enginecontrols.xml</path>
	</model>
	<model>
		<path>Pedestal/Rear/pedestalrear.xml</path>
	</model>
	
	
	<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>30</max-m>
	<max-property>sim/rendering/static-lod/bare</max-property>
	</animation>
  

</PropertyList>
