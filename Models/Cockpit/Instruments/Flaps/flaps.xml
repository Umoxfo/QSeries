<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertyList>
	<path>flaps.ac</path>
	<effect>
		<inherits-from>../../../Effects/panel</inherits-from>
		<object-name>flap.base</object-name>
		<object-name>flap.face</object-name>
		<object-name>flap.needle</object-name>
	</effect>
	<animation>
		<type>material</type>
		<object-name>flap.face</object-name>
		<object-name>flap.needle</object-name>
		<emission>
			<red>   0.50 </red>
			<green> 0.25 </green>
			<blue>  0.25 </blue>
			<factor-prop>systems/electrical/outputs/copflt-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>flap.needle</object-name>
		<property>surface-positions/flap-pos-norm</property>
		<interpolation>
			<entry>
				<ind>   0.00 </ind>
				<dep>   0.0 </dep>
			</entry>
			<entry>
				<ind>  0.1428571 </ind> <!--5-->
				<dep>  30</dep>
			</entry>
			<entry>
				<ind>  0.28571 </ind> <!--10-->
				<dep>  60</dep>
			</entry>
			<entry>
				<ind>  0.428571 </ind> <!--15-->
				<dep>  90</dep>
			</entry>
			<entry>
				<ind>  1 </ind> <!--35-->
				<dep>  210</dep>
			</entry>
		</interpolation>
		<axis>
			<x> -1 </x>
			<y>  0 </y>
			<z> 0 </z>
		</axis>
	</animation>
	
</PropertyList>
