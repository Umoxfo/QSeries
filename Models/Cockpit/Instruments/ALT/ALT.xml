<?xml version="1.0" encoding="ISO-8859-1"?>
<PropertyList>
	<path>ALT.ac</path>
	<effect>
		<inherits-from>../../../Effects/panel</inherits-from>
		<object-name>ALT.screws</object-name>
		<object-name>ALT.base</object-name>
		<object-name>alt.qnh.knob</object-name>
		<object-name>alt.alert.light</object-name>
		
		<object-name>ALT.face</object-name>
		<object-name>alt.needle</object-name>
		<object-name>alt.10000.cover</object-name>
		<object-name>alt.1000.cover</object-name>
		<object-name>alt.10000</object-name>
		<object-name>alt.1000</object-name>
		<object-name>alt.100</object-name>
		<object-name>alt.10</object-name>
		<object-name>inhg.10</object-name>
		<object-name>inhg.1</object-name>
		<object-name>inhg.01</object-name>
		<object-name>inhg.001</object-name>
		<object-name>mb.1000</object-name>
		<object-name>mb.100</object-name>
		<object-name>mb.10</object-name>
		<object-name>mb.1</object-name>
	</effect>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>alt.glass</object-name>
	</effect>
	<animation>
		<type>material</type>
		<object-name>ALT.face</object-name>
		<object-name>alt.needle</object-name>
		<object-name>alt.10000.cover</object-name>
		<object-name>alt.1000.cover</object-name>
		<object-name>alt.10000</object-name>
		<object-name>alt.1000</object-name>
		<object-name>alt.100</object-name>
		<object-name>alt.10</object-name>
		<object-name>inhg.10</object-name>
		<object-name>inhg.1</object-name>
		<object-name>inhg.01</object-name>
		<object-name>inhg.001</object-name>
		<object-name>mb.1000</object-name>
		<object-name>mb.100</object-name>
		<object-name>mb.10</object-name>
		<object-name>mb.1</object-name>
		<emission>
			<red>   0.50 </red>
			<green> 0.25 </green>
			<blue>  0.25 </blue>
			<factor-prop>systems/electrical/outputs/pltflt-norm</factor-prop>
		</emission>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>alt.qnh.knob</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<center>
			<y-m>-0.025891</y-m>
			<z-m>-0.026518</z-m>
		</center>
		<axis>
			<x> -1 </x>
			<y>  0 </y>
			<z>  0 </z>
		</axis>
		<factor>5</factor>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>instrumentation/altimeter/setting-hpa</property>
				<min>950</min>
				<max>1050</max>
				<factor>-1</factor>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>rotate</type>
		<object-name>alt.needle</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.36</factor>
		<axis>
			<x> -1 </x>
			<y>  0 </y>
			<z>  0 </z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>alt.10</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.01</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alt.100</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<factor>0.001</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>select</type>
		<object-name>alt.1000.cover</object-name>
		<condition>
			<less-than>
				<property>instrumentation/altimeter/indicated-altitude-ft</property>
				<value>1000</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alt.1000</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<condition>
			<greater-than-equals>
				<property>instrumentation/altimeter/indicated-altitude-ft</property>
				<value>1000</value>
			</greater-than-equals>
		</condition>
		<factor>0.0001</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>select</type>
		<object-name>alt.10000.cover</object-name>
		<condition>
			<less-than>
				<property>instrumentation/altimeter/indicated-altitude-ft</property>
				<value>10000</value>
			</less-than>
		</condition>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>alt.10000</object-name>
		<property>instrumentation/altimeter/indicated-altitude-ft</property>
		<condition>
			<greater-than-equals>
				<property>instrumentation/altimeter/indicated-altitude-ft</property>
				<value>10000</value>
			</greater-than-equals>
		</condition>
		<factor>0.00001</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<!--QNH indication-->
	<animation>
		<type>textranslate</type>
		<object-name>inhg.10</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.1</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.01</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>1</factor>
		<step>0.1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>inhg.001</object-name>
		<property>instrumentation/altimeter/setting-inhg</property>
		<factor>10</factor>
		<step>0.01</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>textranslate</type>
		<object-name>mb.1000</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<factor>0.0001</factor>
		<step>1000</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>mb.100</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<factor>0.001</factor>
		<step>100</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>mb.10</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<factor>0.01</factor>
		<step>10</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>textranslate</type>
		<object-name>mb.1</object-name>
		<property>instrumentation/altimeter/setting-hpa</property>
		<factor>0.1</factor>
		<step>1</step>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
</PropertyList>
