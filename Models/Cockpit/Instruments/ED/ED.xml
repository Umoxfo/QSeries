<?xml version="1.0"?>

<PropertyList>

	<path>ED.ac</path>
	
	<animation>
		<type>pick</type>
		<object-name>ED.screen</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>canvas_ed.showED();</script>
			</binding>
		</action>
	</animation>
		
	<animation>
		<type>material</type>
		<object-name>ED.screen</object-name>
		<ambient>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>instrumentation/ED/brightness-norm</factor-prop>
		</ambient>
		<diffuse>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>instrumentation/ED/brightness-norm</factor-prop>
		</diffuse>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>1</blue>
			<factor-prop>instrumentation/ED/brightness-norm</factor-prop>
		</emission>
	</animation>


</PropertyList>
