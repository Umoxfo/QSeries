		</binding>
		</hovered>
	</animation>
	<!--Opposite direction-->
	<animation>
		<type>pick</type>
		<object-name>flapslever</object-name>
		<action>
			<button>1</button>
			<repeatable>0</repeatable>
			<binding>
				<command>property-cycle</command>
				<property>controls/flight/flaps</property>
				<value>1.000</value>
				<value>0.426</value>
				<value>0.284</value>
				<value>0.142</value>
				<value>0.000</value>
			</binding>
		</action>
		<hovered>
