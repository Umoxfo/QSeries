		<type>rotate</type>
		<object-name>throttleR</object-name>
		<property>controls/engines/engine[1]/throttle-int</property>
		<condition>
			<property>/controls/engines/engine[1]/reverser</property>
		</condition>
		<factor>30.5</factor>
		<center>
			<x-m>-11.90089</x-m>
			<y-m>0</y-m>
			<z-m>1.86985</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
    
	<!--Condition Levers-->
	<animation>
		<type>pick</type>
		<object-name>conditionL</object-name>
		<action>
			<button>0</button>
		<binding>
		<command>property-cycle</command>
			<property>controls/engines/engine[0]/condition-lever-state</property>
			<value>1</value><!--START&FEATHER-->
			<value>2</value><!--MIN 850-->
			<value>3</value><!--900-->
			<value>4</value><!--MAX 1020-->
			<value>0</value><!--FUEL OFF-->
		</binding>
		</action>
		<action>
			<button>1</button>
		<binding>
		<command>property-cycle</command>
			<property>controls/engines/engine[0]/condition-lever-state</property>
			<value>0</value><!--FUEL OFF-->
			<value>4</value><!--MAX 1020-->
			<value>3</value><!--900-->
			<value>2</value><!--MIN 850-->
			<value>1</value><!--START&FEATHER-->
		</binding>
		</action>
		<hovered>
		<binding>
		<command>set-tooltip</command>
		<label>LH Condition Lever: %s</label>
		<tooltip-id>lh-condition</tooltip-id>
			<property>controls/engines/engine[0]/condition-lever-state</property>
			<mapping>nasal</mapping>
			<script>
				var modes = ['FUEL OFF', 'START AND FEATHER', 'MIN 850', '900', 'MAX 1020'];
				return modes[arg[0]];
			</script>
		</binding>
		</hovered>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>conditionL</object-name>
		<property>controls/engines/engine[0]/condition-lever-pos</property>
		<interpolation>
			<entry>
				<ind> 0 </ind>
				<dep>  0 </dep>
			</entry>
			<entry>
				<ind> 1 </ind>
				<dep>  -25 </dep>
			</entry>
			<entry>
				<ind> 2 </ind>
				<dep> -50 </dep>
			</entry>
			<entry>
				<ind> 3 </ind>
				<dep> -95 </dep>
			</entry>
			<entry>
				<ind> 4 </ind>
				<dep> -125 </dep>
			</entry>
		</interpolation>
		<center>
		<x-m>-11.88406</x-m>
		<y-m>0</y-m>
		<z-m>1.92495</z-m>
		</center>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
