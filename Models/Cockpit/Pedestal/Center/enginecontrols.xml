<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>enginecontrols.ac</path>
	
	<!--Throttles-->

	<animation>
		<type>slider</type>
		<object-name>throttleL</object-name>
		<drag-direction>vertical</drag-direction>
		<action>
		<binding>
		<command>property-adjust</command>
		<property>controls/engines/engine[0]/throttle</property>
		<factor>0.05</factor>
		<min>-1</min>
		<max>1</max>
		<wrap>0</wrap>
		</binding>
		</action>
		<hovered>
		<binding>
		<command>set-tooltip</command>
		<label>LH Throttle: %3d%%</label>
		<measure-text>LH Throttle: 100%</measure-text>
		<tooltip-id>lh-throttle</tooltip-id>
		<mapping>percent</mapping>
		<property>controls/engines/engine[0]/throttle-int</property>
		</binding>
		</hovered>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/throttle-int</property>
		<condition>
		<not>
			<property>/controls/engines/engine[0]/reverser</property>
		</not>
		</condition>
		<factor>-41.24</factor>
		<center>
			<x-m>-11.90089</x-m>
			<y-m>0</y-m>
			<z-m>1.86985</z-m>
		</center>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/reverser-position</property>
		<factor>14</factor>
		<center>
			<x-m>-11.90089</x-m>
			<y-m>0</y-m>
			<z-m>1.86985</z-m>
		</center>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/throttle-int</property>
		<condition>
			<property>/controls/engines/engine[0]/reverser</property>
		</condition>
		<factor>30.5</factor>
		<center>
			<x-m>-11.90089</x-m>
			<y-m>0</y-m>
			<z-m>1.86985</z-m>
		</center>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>throttleR</object-name>
		<drag-direction>vertical</drag-direction>
		<action>
		<binding>
		<command>property-adjust</command>
		<property>controls/engines/engine[1]/throttle</property>
		<factor>0.05</factor>
