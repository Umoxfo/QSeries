<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
	<path>enginecontrols-200.ac</path>
	
	<!--Throttles-->

	<animation>
		<type>slider</type>
		<object-name>throttleL</object-name>
		<drag-direction>vertical</drag-direction>
		<action>
		<binding>
		<command>property-adjust</command>
		<property>controls/engines/engine[0]/throttle</property>
		<factor>0.05</factor>
		<min>-1</min>
		<max>1</max>
		<wrap>0</wrap>
		</binding>
		</action>
		<hovered>
		<binding>
		<command>set-tooltip</command>
		<label>LH Throttle: %3d%%</label>
		<measure-text>LH Throttle: 100%</measure-text>
		<tooltip-id>lh-throttle</tooltip-id>
		<mapping>percent</mapping>
		<property>controls/engines/engine[0]/throttle-int</property>
		</binding>
		</hovered>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/throttle-int</property>
		<condition>
		<not>
			<property>/controls/engines/engine[0]/reverser</property>
		</not>
		</condition>
		<factor>-48</factor>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/reverser-position</property>
		<factor>14</factor>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleL</object-name>
		<property>controls/engines/engine[0]/throttle-int</property>
		<condition>
			<property>/controls/engines/engine[0]/reverser</property>
		</condition>
		<factor>14</factor>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	
	<animation>
		<type>slider</type>
		<object-name>throttleR</object-name>
		<drag-direction>vertical</drag-direction>
		<action>
		<binding>
		<command>property-adjust</command>
		<property>controls/engines/engine[1]/throttle</property>
		<factor>0.05</factor>
		<min>-1</min>
		<max>1</max>
		<wrap>0</wrap>
		</binding>
		</action>
		<hovered>
		<binding>
		<command>set-tooltip</command>
		<label>RH Throttle: %3d%%</label>
		<measure-text>RH Throttle: 100%</measure-text>
		<tooltip-id>rh-throttle</tooltip-id>
		<mapping>percent</mapping>
		<property>controls/engines/engine[1]/throttle-int</property>
		</binding>
		</hovered>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleR</object-name>
		<property>controls/engines/engine[1]/throttle-int</property>
		<condition>
		<not>
			<property>/controls/engines/engine[1]/reverser</property>
		</not>
		</condition>
		<factor>-48</factor>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleR</object-name>
		<property>controls/engines/engine[1]/reverser-position</property>
		<factor>14</factor>
		<axis>
		<x>0</x>
		<y>1</y>
		<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>throttleR</object-name>
		<property>controls/engines/engine[1]/throttle-int</property>
		<condition>
			<property>/controls/engines/engine[1]/reverser</property>
		</condition>
		<factor>14</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
    
	<!--Condition Levers-->
	<!--Concept:
		set FUEL OFF/START&FEATHER/NORMAL (condition-lever-state 0/1/2)
		free movement between MIN (900) to MAX (1020) (condition-lever-set 0-1)
	-->
	<animation>
		<type>knob</type>
		<object-name>conditionL</object-name>
		<action>
			<binding>
				<condition>
					<equals>
						<property>controls/engines/engine[0]/condition-lever-state</property>
						<value>2</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>controls/engines/engine[0]/condition-lever-set</property>
				<min>0</min>
				<max>1</max>
				<factor>-0.05</factor>
				<wrap>false</wrap>
			</binding>
		</action>
		<hovered>
			<binding>
				<condition>
					<not-equals>
						<property>controls/engines/engine[0]/condition-lever-state</property>
						<value>2</value>
					</not-equals>
				</condition>
				<command>set-tooltip</command>
				<label>LH Condition: %s</label>
				<tooltip-id>lh-condition</tooltip-id>
				<property>controls/engines/engine[0]/condition-lever-state</property>
				<mapping>nasal</mapping>
				<script>
					var modes = ['FUEL OFF', 'START AND FEATHER', 'NORM'];
					return modes[arg[0]];
				</script>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>controls/engines/engine[0]/condition-lever-state</property>
						<value>2</value>
					</equals>
				</condition>
				<command>set-tooltip</command>
				<label>LH Condition: RPM %4d</label>
				<tooltip-id>lh-condition2</tooltip-id>
				<property>controls/engines/engine[0]/condition-lever-set</property>
				<mapping>nasal</mapping>
				<script>
					return (900+300*arg[0]);
				</script>
			</binding>
		</hovered>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>conditionL.fueloff</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[0]/condition-lever-state</property>
				<value>0</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>conditionL.startfeather</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[0]/condition-lever-state</property>
				<value>1</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>conditionL.min</object-name>
		<visible>0</visible>
		<action>
			<button>0</button>
			<binding>
				<command>property-assign</command>
				<property>controls/engines/engine[0]/condition-lever-state</property>
				<value>2</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>conditionL</object-name>
		<property>controls/engines/engine[0]/condition-lever-pos</property>
		<interpolation>
			<entry>
				<ind> 0 </ind>
				<dep>  0 </dep>
			</entry>
			<entry>
				<ind> 1 </ind>
				<dep>  -16.9 </dep>
			</entry>
			<entry>
				<ind> 2 </ind>
				<dep> -52.72 </dep>
			</entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>conditionL</object-name>
		<condition>
			<equals>
				<property>controls/engines/engine[0]/condition-lever-state</property>
				<value>2</value>
			</equals>
		</condition>
		<property>controls/engines/engine[0]/condition-lever-set</property>
		<factor>-20.62</factor>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>
