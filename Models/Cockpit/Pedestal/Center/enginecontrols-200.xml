	<animation>
		<type>pick</type>
		<object-name>throttlelock</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>/controls/engines/throttle-lock</property>
