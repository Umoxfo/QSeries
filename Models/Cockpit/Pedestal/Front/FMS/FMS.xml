<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

    <path>FMS.ac</path>
    
    
	<!--Interior effect-->
	<effect>
		<inherits-from>../../../../Effects/interior/interior-shadow</inherits-from>
		<object-name>base</object-name>
	</effect>

	<animation>
		<type>pick</type>
		<object-name>onoffdim.btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-toggle</command>
				<property>instrumentation/FMS/onoff</property>
			</binding>
		</action>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>next.btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>canvas_fms.nextPage();</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>prev.btn</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>canvas_fms.previousPage();</script>
			</binding>
		</action>
	</animation>


</PropertyList>
