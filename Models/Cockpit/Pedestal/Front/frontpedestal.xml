<?xml version="1.0" encoding="UTF-8"?>                                                                                                                                                                                                      
<PropertyList>                                                                                                                                                                                                                              
        <path>frontpedestal.ac</path>                                                                                                                                                                                                       
        <!--Panel illumination-->                                                                                                                                                                                                           
        <effect>                                                                                                                                                                                                                            
                <inherits-from>Aircraft/Q400/Models/Effects/panel-lighting</inherits-from>                                                                                                                                                  
                <parameters>                                                                                                                                                                                                                
                        <implicit-lightmap-intensity>                                                                                                                                                                                       
                                <use>/systems/electrical/outputs/enginepanel-lights</use>                                                                                                                                                   
                        </implicit-lightmap-intensity>                                                                                                                                                                                      
                </parameters>                                                                                                                                                                                                               
                <object-name>base1</object-name>                                                                                                                                                                                            
                <object-name>base2</object-name>                                                                                                                                                                                            
                <object-name>base3</object-name>                                                                                                                                                                                            
                <object-name>base4</object-name>                                                                                                                                                                                            
                <object-name>base5</object-name>                                                                                                                                                                                            
                <object-name>screws1</object-name>                                                                                                                                                                                          
                <object-name>screws2</object-name>                                                                                                                                                                                          
                <object-name>screws3</object-name>                                                                                                                                                                                          
                <object-name>screws4</object-name>                                                                                                                                                                                          
                <object-name>screws5</object-name>                                                                                                                                                                                          
                <object-name>cover1</object-name>                                                                                                                                                                                           
                <object-name>cover2</object-name>                                                                                                                                                                                           
                <object-name>NDtcasL</object-name>                                                                                                                                                                                          
                <object-name>NDcenteredL</object-name>                                                                                                                                                                                      
                <object-name>NDwxterrL</object-name>                                                                                                                                                                                        
                <object-name>NDdataL</object-name>                                                                                                                                                                                          
                <object-name>NDtcasR</object-name>                                                                                                                                                                                          
                <object-name>NDcenteredR</object-name>                                                                                                                                                                                      
                <object-name>NDwxterrR</object-name>                                                                                                                                                                                        
                <object-name>NDdataR</object-name>                                                                                                                                                                                          
        </effect>                                                                                                                                                                                                                           
        <effect>                                                                                                                                                                                                                            
                <inherits-from>Effects/model-transparent</inherits-from>                                                                                                                                                                    
                <object-name>cover1glass</object-name>                                                                                                                                                                                      
                <object-name>cover2glass</object-name>                                                                                                                                                                                      
        </effect>                                                                                                                                                                                                                           
        <!--Left EFIS Control Panel-->                                                                                                                                                                                                      
        <animation>                                                                                                                                                                                                                         
                <type>pick</type>                                                                                                                                                                                                           
                <object-name>NDtcasL</object-name>                                                                                                                                                                                          
                <action>                                                                                                                                                                                                                    
                        <button>0</button>                                                                                                                                                                                                  
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis/inputs/tfc</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDcenteredL</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis/inputs/nd-centered</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDwxterrL</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis/inputs/wxr</property>
                        </binding>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis/inputs/terr</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDdataL</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-cycle</command>
                                <property>/instrumentation/efis[0]/inputs/data-btn</property>
                                <value>1</value>
                                <value>2</value>
                                <value>3</value>
                                <value>4</value>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>knob</type>
                <object-name>PFD0.brt</object-name>
                <action>
                        <binding>
                                <command>property-adjust</command>
                                <property>/instrumentation/pfd[0]/brt</property>
                                <factor>0.05</factor>
                                <min>0.0</min>
                                <max>1.0</max>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>knob</type>
                <object-name>MFD0.brt</object-name>
                <action>
                        <binding>
                                <command>property-adjust</command>
                                <property>/instrumentation/mfd[0]/brt</property>
                                <factor>0.05</factor>
                                <min>0.0</min>
                                <max>1.0</max>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>knob</type>
                <object-name>NDrangeL</object-name>
                <action>
                        <binding>
                                <command>property-cycle</command>
                                <property>/instrumentation/efis/inputs/range-nm</property>
                                <value>20</value>
                                <value>40</value>
                                <value>80</value>
                                <value>160</value>
                                <value>240</value>
                                <value>10</value>
                        </binding>
                </action>
        </animation>
        <!--Right EFIS Control Panel-->
        <animation>
                <type>pick</type>
                <object-name>NDtcasR</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis[1]/inputs/tfc</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDcenteredR</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis[1]/inputs/nd-centered</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDwxterrR</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis/inputs/wxr</property>
                        </binding>
                        <binding>
                                <command>property-toggle</command>
                                <property>/instrumentation/efis[1]/inputs/terr</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>NDdataR</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-cycle</command>
                                <property>/instrumentation/efis[1]/inputs/data-btn</property>
                                <value>1</value>
                                <value>2</value>
                                <value>3</value>
                                <value>4</value>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>knob</type>
                <object-name>PFD1.brt</object-name>
                <action>
                        <binding>
                                <command>property-adjust</command>
                                <property>/instrumentation/pfd[1]/brt</property>
                                <factor>0.05</factor>
                                <min>0.0</min>
                                <max>1.0</max>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>knob</type>
                <object-name>MFD1.brt</object-name>
                <action>
                        <binding>
                                <command>property-adjust</command>
                                <property>/instrumentation/mfd[1]/brt</property>
                                <factor>0.05</factor>
                                <min>0.0</min>
                                <max>1.0</max>
                        </binding>
                </action>
        </animation>
        <!--ENGINE CONTROL PANEL-->
        <animation>
                <type>pick</type>
                <object-name>MTOPDiscrete</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/controls/engines/mtop-discrete</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>textranslate</type>
                <object-name>MTOPDiscrete</object-name>
                <property>/controls/engines/mtop-discrete</property>
                <axis>
                        <x>1</x>
                </axis>
                <factor>0.25</factor>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>Tank1AuxPump</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/controls/fuel/tank1-aux-pump</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>textranslate</type>
                <object-name>Tank1AuxPump</object-name>
                <property>/controls/fuel/tank1-aux-pump</property>
                <axis>
                        <x>1</x>
                </axis>
                <factor>0.25</factor>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>Tank2AuxPump</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-toggle</command>
                                <property>/controls/fuel/tank2-aux-pump</property>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>textranslate</type>
                <object-name>Tank2AuxPump</object-name>
                <property>/controls/fuel/tank2-aux-pump</property>
                <axis>
                        <x>1</x>
                </axis>
                <factor>0.25</factor>
        </animation>
        <animation>
                <type>pick</type>
                <object-name>fueltransfer</object-name>
                <action>
                        <button>0</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-adjust</command>
                                <property>/controls/fuel/transfer</property>
                                <step>1</step>
                                <min>-1</min>
                                <max>1</max>
                        </binding>
                </action>
                <action>
                        <button>1</button>
                        <repeatable>0</repeatable>
                        <binding>
                                <command>property-adjust</command>
                                <property>/controls/fuel/transfer</property>
                                <step>-1</step>
                                <min>-1</min>
                                <max>1</max>
                        </binding>
                </action>
        </animation>
        <animation>
                <type>rotate</type>
                <object-name>fueltransfer</object-name>
                <property>/controls/fuel/transfer</property>
                <factor>-30</factor>
                <axis>
                        <x>1</x>
                </axis>
                <center>
                        <y-m> -0.00297</y-m>
                        <z-m>  1.98525</z-m>
                </center>
        </animation>
</PropertyList>

