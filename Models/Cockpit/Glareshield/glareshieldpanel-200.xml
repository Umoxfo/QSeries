	<!--NAV Button-->
	<animation>
		<type>pick</type>
		<object-name>nav</object-name>
		<object-name>nav.lightmap</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<equals>
						<property>/it-autoflight/settings/nav-source</property>
						<value>FMS</value>
					</equals>
				</condition>
				<command>property-assign</command>
				<property>it-autoflight/input/lat</property>
				<value>1</value>
			</binding>
			<binding>
				<condition>
					<or>
						<equals>
							<property>/it-autoflight/settings/nav-source</property>
							<value>NAV1</value>
						</equals>
						<equals>
							<property>/it-autoflight/settings/nav-source</property>
							<value>NAV2</value>
						</equals>
					</or>
				</condition>
				<command>property-assign</command>
				<property>it-autoflight/input/lat</property>
				<value>2</value>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/backcourse-trk</property>
				<value>0</value>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/nav-pressed</property>
			</binding>
			<mod-up>
				<binding>
					<command>property-toggle</command>
					<property>it-autoflight/input/nav-pressed</property>
				</binding>
			</mod-up>
		</action>
	</animation>
