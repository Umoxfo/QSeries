	<!--ALT SEL Button-->
	<animation>
		<type>pick</type>
		<object-name>altsel</object-name>
		<object-name>altsel.lightmap</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>setprop("it-autoflight/input/alt", math.round(getprop("position/altitude-ft"), 500));</script>
			</binding>
			<binding>
				<command>property-toggle</command>
				<property>it-autoflight/input/altsel-pressed</property>
			</binding>
			<mod-up>
				<binding>
					<command>property-toggle</command>
					<property>it-autoflight/input/altsel-pressed</property>
				</binding>
			</mod-up>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>altsel</object-name>
		<object-name>altsel.lightmap</object-name>
		<property>it-autoflight/input/altsel-pressed</property>
		<axis>
			<x>1</x>
		</axis>
		<factor>-0.0015</factor>
	</animation>
	<!--Vertical Speed Wheel-->
	<animation>
		<type>slider</type>
		<object-name>vsWheel</object-name>
		<drag-direction>vertical</drag-direction>
		<action>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/input/vert/</property>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/vs</property>
				<factor>100</factor>
				<min>-5000</min>
				<max>5000</max>
				<wrap>false</wrap>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>it-autoflight/input/vert/</property>
						<value>4</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>it-autoflight/input/spd-kts</property>
				<factor>-5</factor>
				<min>120</min>
				<max>300</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>hdgL</object-name>
		<object-name>hdgR</object-name>
		<action>
			<binding>
				<command>property-adjust</command>
				<property>it-autoflight/input/hdg</property>
				<factor>1</factor>
				<min>0</min>
				<max>360</max>
				<wrap>true</wrap>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<object-name>hsisel</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-assign</command>
				<property>it-autoflight/input/hdg</property>
				<property>/orientation/heading-magnetic-deg</property>
			</binding>
		</action>
	</animation>
	
	<!--Collins NAV-->
	<animation>
		<type>knob</type>
		<object-name>nav0.master</object-name>
		<property>instrumentation/nav[0]/power-btn</property>
		<factor>-33</factor>
		<center>
			<y-m>-0.121188</y-m>
			<z-m>-0.004544</z-m>
		</center>
		<axis>
			<x>1</x>
		</axis>
		<action>
