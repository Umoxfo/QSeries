				<command>property-toggle</command>
				<property>instrumentation/nav[0]/power-btn</property>
			</binding>
		</action>
	</animation>
	<animation>
		<type>knob</type>
		<object-name>nav0.mhz</object-name>
		<property>instrumentation/nav[0]/frequencies/standby-mhz</property>
		<factor>-0.25</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
		<center>
			<y-m>-0.094386</y-m>
			<z-m>-0.002349</z-m>
		</center>
		<action>
		<binding>
			<condition>
				<property>instrumentation/nav[0]/power-btn</property>
			</condition>
			<command>property-adjust</command>
			<property>instrumentation/nav[0]/frequencies/standby-mhz</property>
