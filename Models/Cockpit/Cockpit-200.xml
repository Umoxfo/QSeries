<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <path>cockpit-200.ac</path>

<animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>10</max-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>
  
  
    <effect>
        <inherits-from>Aircraft/QSeries/Models/Effects/panel</inherits-from>
        <object-name>tillersteerL</object-name>
        <object-name>tillersteerR</object-name>
		<object-name>panel.003</object-name>
		<object-name>seatP1</object-name>
		<object-name>seatP2</object-name>
		<object-name>seatCP1</object-name>
		<object-name>seatCP2</object-name>
		<object-name>somepanel</object-name>
    </effect>
	<effect>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>hud.glass</object-name>
	</effect>
	
    <!--Parts-->
    <model>
	    <path>Panels-200.xml</path>
    </model>
<!--Tiller Steering-->
    <animation>
        <type>rotate</type>
        <object-name>tillersteerL</object-name>
        <property>gear/gear/caster-angle-deg</property>
        <factor>-0.15</factor>
		<axis>
            <x1-m>-11.67347</x1-m>
            <y1-m>-0.87988</y1-m>
            <z1-m>1.85555</z1-m>
            <x2-m>-11.61158</x2-m>
            <y2-m>-0.87988</y2-m>
            <z2-m>1.89348</z2-m>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>tillersteerR</object-name>
        <property>gear/gear/caster-angle-deg</property>
        <factor>-0.15</factor>
		<axis>
            <x1-m>-11.67347</x1-m>
            <y1-m>0.87988</y1-m>
            <z1-m>1.85555</z1-m>
            <x2-m>-11.61158</x2-m>
            <y2-m>0.87988</y2-m>
            <z2-m>1.89348</z2-m>
        </axis>
    </animation>
	
</PropertyList>
